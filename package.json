      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/helper-builder-react-jsx": "^7.16.7",
    "@types/enzyme": "^3.10.11",
    "enzyme": "^3.11.0",
